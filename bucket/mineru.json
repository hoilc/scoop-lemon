        [
            "MinerU.exe",
            "MinerU",
            "--user-data-dir=\"User Data\""
        ]
    ],
    "persist": "User Data",
    "checkver": {
        "url": "https://webpub.shlab.tech/MinerU/latest/win/latest.yml",
        "regex": "version: ([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://webpub.shlab.tech/MinerU/latest/win/MinerU-$version-setup.exe#/dl.7z"
