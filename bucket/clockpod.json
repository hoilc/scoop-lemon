{
    "version": "2.72p1",
    "description": "A clock and other information display tool that stick to the window in use.",
    "homepage": "http://toro.d.dooo.jp/slwin4.html#cpod",
    "license": "Freeware",
    "architecture": {
        "64bit": {
            "shortcuts": [
                [
                    "CLOCKPOD64.EXE",
                    "ClockPod"
                ]
            ]
        },
        "32bit": {
            "shortcuts": [
                [
                    "CLOCKPOD.EXE",
                    "ClockPod"
                ]
            ]
        }
    },
    "url": "http://toro.d.dooo.jp/clockpod272p1.zip",
    "hash": "8cc5dcba12a600ce6b42096735d428c5a38af964fba6b2d90fa32cc649a095db",
    "persist": "CLOCKPOD.CFG",
    "checkver": {
        "regex": "clockpod(?<major>\\d)(?<rest>.+).zip\"",
        "replace": "${major}.${rest}",
        "reverse": true
    },
    "autoupdate": {
        "url": "http://toro.d.dooo.jp/clockpod$cleanVersion.zip"
    }
}
