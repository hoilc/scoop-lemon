{
    "version": "0.4.0",
    "description": "LuminS is a fast and reliable alternative to rsync for synchronizing local files.",
    "homepage": "https://github.com/wchang22/LuminS",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/cargo-bins/cargo-quickinstall/releases/download/lms-0.4.0/lms-0.4.0-x86_64-pc-windows-msvc.tar.gz",
            "hash": "0f986bfb38a557d45170bc70f5eace4b8b836d715ed26891bdd343786a3c38f9"
        },
        "arm64": {
            "url": "https://github.com/cargo-bins/cargo-quickinstall/releases/download/lms-0.4.0/lms-0.4.0-aarch64-pc-windows-msvc.tar.gz",
            "hash": "77b1e1b2e76ba043f18966e61326f217553f7e02237edec675ad50d020d84085"
        }
    },
    "bin": "lms.exe",
    "checkver": {
        "url": "https://crates.io/api/v1/crates/lms?include=default_version",
        "jsonpath": "$.crate.default_version"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
