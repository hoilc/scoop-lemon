    "checkver": {
        "script": [
            "$branch = Invoke-RestMethod 'https://api.github.com/repos/SagerNet/sing-geoip/branches/rule-set'",
            "return ([DateTime][String]$branch.commit.commit.committer.date).ToUniversalTime().ToString(\"yyyyMMdd\") + ' ' + $branch.commit.sha"
